{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/claim",
        "destination": "/claim.html"
      },
      {
        "source": "/claim-detail",
        "destination": "/claim-detail.html"
      },
      {
        "source": "/admin",
        "destination": "/admin.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
